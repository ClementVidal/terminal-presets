{
    "name": "terminal-presets",
    "displayName": "Terminal presets",
    "description": "Quickly create presets of preconfigured terminals (including name, icon, color...)",
    "publisher": "clement-vidal-vscode",
    "version": "1.2.0",
    "icon": "icon.png",
    "keywords": [
        "terminal",
        "monorepo",
        "presets",
        "config"
    ],
    "repository": {
        "url": "https://github.com/ClementVidal/terminal-presets.git"
    },
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:terminal-presets.terminal-presets"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "terminal-presets.terminal-presets",
                "title": "Terminal presets"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run package",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "lint": "prettier --write ."
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/node": "14.x",
        "@types/vscode": "^1.60.0",
        "@typescript-eslint/eslint-plugin": "^4.26.0",
        "@typescript-eslint/parser": "^4.26.0",
        "eslint": "^7.27.0",
        "glob": "^7.1.7",
        "prettier": "^2.4.1",
        "ts-loader": "^9.2.2",
        "typescript": "^4.3.2",
        "vscode-test": "^1.5.2",
        "webpack": "^5.38.1",
        "webpack-cli": "^4.7.0"
    }
}
